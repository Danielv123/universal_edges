{
	"extends": "./tsconfig.base.json",
	"include": [
		"./**/*.ts"
	],
	"exclude": [
		"test/*",
		"./dist/*"
	],
	"compilerOptions": {
		"lib": [
			"es2023"
		],
		"module": "node16",
		"moduleResolution": "node16",
		"target": "es2022",
		"forceConsistentCasingInFileNames": true,
		"outDir": "dist/node",
	},
}